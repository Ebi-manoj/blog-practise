<div class="container-fluid header">
  <h1 class="text-center mt-5">Hi,I am Ebi Manoj!</h1>
  <h4 class="text-center my-4">MERN Stack Aspirer</h4>
  <img
    class="header-img img-fluid w-50 d-flex mx-auto"
    src="/img/header-img.jpg"
    alt="header-Img"
  />
</div>

<article class="article container-fluid">
  <h3 class="my-5">LATEST POST</h3>
  <ul class="articles_ul">
    <% data.forEach(post => { %>

    <li>
      <div class="li-content">
        <a href="#">
          <span><%= post.title %></span
          ><span class="article_date"
            ><%= post.createdAt.toDateString() %></span
          >
        </a>
      </div>
    </li>
    <% }) %>
  </ul>
  <% if (hasNextPage!==null) { %>

  <a href="/?page=<%=nextPage%>" class="pagination">&lt; View Older</a>
  <% } %>
</article>
