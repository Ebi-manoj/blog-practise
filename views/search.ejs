<article class="article container-fluid">
  <h3 class="my-5">
    <%= data&&data.length?'Search Result':'No Search Result' %>
  </h3>
  <ul class="articles_ul">
    <% data.forEach(post => { %>

    <li>
      <div class="li-content">
        <a href="/post/<%= post._id %>">
          <span><%= post.title %></span
          ><span class="article_date"
            ><%= post.createdAt.toDateString() %></span
          >
        </a>
      </div>
    </li>
    <% }) %>
  </ul>
</article>
